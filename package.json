{
  "name": "mrashidi.me",
  "version": "0.1.67",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "NEXT_TELEMETRY_DISABLED=1 pnpm generate-sitemap && next build",
    "build:fast": "pnpm generate-sitemap && SKIP_PWA=true next build",
    "start": "next start",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "generate-sitemap": "NODE_NO_WARNINGS=1 tsx scripts/generate-sitemap.ts",
    "check-og-images": "node scripts/generate-og-images.js",
    "generate-og-images": "node scripts/generate-placeholder-og.js",
    "analyze": "ANALYZE=true next build",
    "release": "npx standard-version",
    "clean": "pnpm store prune && rm -rf node_modules .next",
    "reinstall": "pnpm clean && pnpm install",
    "update": "pnpm update --latest",
    "outdated": "pnpm outdated"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.896.0",
    "@fontsource/albert-sans": "^5.2.8",
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@fontsource/orbitron": "^5.2.8",
    "@fontsource/press-start-2p": "^5.2.7",
    "@fontsource/vt323": "^5.2.7",
    "@headlessui/react": "^2.2.9",
    "@hookform/resolvers": "^5.2.2",
    "@next/third-parties": "^15.5.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-email/components": "^0.5.5",
    "@react-email/render": "^1.3.1",
    "@sentry/nextjs": "^10.15.0",
    "@types/multer": "^2.0.0",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.4",
    "@use-gesture/react": "^10.3.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^2.0.0",
    "@vercel/speed-insights": "^1.2.0",
    "canvas": "^3.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "file-type": "^21.0.0",
    "framer-motion": "^12.23.22",
    "helmet": "^8.1.0",
    "lucide-react": "^0.544.0",
    "minimatch": "^10.0.3",
    "multer": "^2.0.2",
    "next": "^15.5.4",
    "next-pwa": "^5.6.0",
    "node-cache": "^5.1.2",
    "pino": "^9.12.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-email": "^4.2.12",
    "react-hook-form": "^7.63.0",
    "react-icons": "^5.5.0",
    "sanitize-filename": "^1.6.3",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "xml2js": "^0.6.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@next/bundle-analyzer": "^15.5.4",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.8",
    "@types/xml2js": "^0.4.14",
    "@vitejs/plugin-react": "^5.0.4",
    "fast-glob": "^3.3.3",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.3",
    "pino-pretty": "^13.1.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.6",
    "typescript": "5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a",
  "engines": {
    "node": "22.x",
    "pnpm": "10.x"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "biome check --write",
      "biome format --write"
    ],
    "scripts/**/*.{js,ts}": [
      "biome check --write",
      "biome format --write"
    ],
    "*.json": [
      "biome format --write"
    ],
    "src/**/*.{css,scss}": [
      "biome check --write"
    ]
  }
}
